<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.shape.Polygon?>
<?import java.lang.*?>
<Pane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">

    <Polygon strokeWidth="0.1" stroke="black" fill="lightgray">
        <points>
            <Double fx:value="0.0"/><Double fx:value="0.0"/>
            <Double fx:value="1.0"/><Double fx:value="0.0"/>
            <Double fx:value="1.0"/><Double fx:value="0.4"/>
            <Double fx:value="3.0"/><Double fx:value="0.4"/>
            <Double fx:value="3.0"/><Double fx:value="0.0"/>
            <Double fx:value="4.0"/><Double fx:value="0.0"/>
            <Double fx:value="4.0"/><Double fx:value="4.0"/>
            <Double fx:value="0.0"/><Double fx:value="4.0"/>
            <Double fx:value="0.0"/><Double fx:value="3.0"/>
            <Double fx:value="0.4"/><Double fx:value="2.0"/>
            <Double fx:value="0.0"/><Double fx:value="1.0"/>
        </points>
    </Polygon>

    <Line strokeWidth="0.1" stroke="black" startX="0" startY="2" endX="0.4" endY="2"/>
    <Line strokeWidth="0.1" stroke="black" startX="2" startY="0" endX="2" endY="0.4"/>

    <Circle fx:id="body" strokeWidth="0.1" stroke="black" centerX="2" centerY="2" radius="1.3"/>

</Pane>
