<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Polygon?>
<?import java.lang.*?>
<Pane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" prefWidth="4" prefHeight="10">

    <Polygon strokeWidth="0.1" stroke="black" fill="lightgray">
        <points>
            <Double fx:value="0"/><Double fx:value="0"/>
            <Double fx:value="3"/><Double fx:value="0"/>
            <Double fx:value="4"/><Double fx:value="1"/>
            <Double fx:value="4"/><Double fx:value="9"/>
            <Double fx:value="3"/><Double fx:value="10"/>
            <Double fx:value="0"/><Double fx:value="10"/>
            <Double fx:value="0"/><Double fx:value="6"/>
            <Double fx:value="0.5"/><Double fx:value="5"/>
            <Double fx:value="0"/><Double fx:value="4"/>
            <Double fx:value="0"/><Double fx:value="0"/>
        </points>
    </Polygon>

    <Label fx:id="name" layoutX="1" layoutY="0.5" minWidth="3" maxWidth="3" minHeight="9" maxHeight="9"
           textFill="black" style="-fx-font-size: 1px" textAlignment="CENTER"/>

</Pane>
