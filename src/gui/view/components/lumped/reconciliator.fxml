<?xml version="1.0" encoding="UTF-8"?>

<!--suppress ALL -->
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.shape.Polygon?>
<?import java.lang.*?>
<Pane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" prefWidth="2" prefHeight="2">

    <!--wires-->
    <Line stroke="black" strokeWidth="0.1" startX="0" startY="1" endX="0.5" endY="1"/>
    <Line stroke="black" strokeWidth="0.1" startX="1.5" startY="1" endX="2" endY="1"/>

    <!--body-->
    <Polygon fx:id="body" stroke="black" strokeWidth="0.1">
        <points>
            <Double fx:value="0.5"/> <Double fx:value="0.5"/>
            <Double fx:value="0.5"/> <Double fx:value="1.5"/>
            <Double fx:value="1.0"/> <Double fx:value="1.5"/>
            <Double fx:value="1.5"/> <Double fx:value="1.0"/>
            <Double fx:value="1.0"/> <Double fx:value="0.5"/>
        </points>
    </Polygon>

</Pane>
